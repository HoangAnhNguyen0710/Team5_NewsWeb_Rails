<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= article.category.category_name %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= article.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= article.user.email %>
  </p>

  <p>
    <strong>Display:</strong>
    <%= article.display %>
  </p>

</div>
